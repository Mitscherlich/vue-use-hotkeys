{
  "name": "vue-use-hotkeys",
  "description": "Vue composition for using keyboard shortcuts in components",
  "version": "0.3.3",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "source": "./src/index.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mitscherlich/vue-use-hotkeys.git"
  },
  "homepage": "https://github.com/Mitscherlich/vue-use-hotkeys#readme",
  "bugs": "https://github.com/Mitscherlich/vue-use-hotkeys/issues",
  "author": "Mitscherlich <mitscherlich36@gmail.com>",
  "scripts": {
    "build": "tsup ./src/index.ts",
    "dev": "tsup --watch",
    "test": "vitest run",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp package.json --commit --push --tag && pnpm publish"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-rc.1",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "@m9ch/vhooks": "^0.1.0-alpha.11",
    "hotkeys-js": "^3.9.4",
    "vue-demi": "^0.13.0"
  },
  "devDependencies": {
    "@m9ch/eslint-config-all": "0.2.3",
    "@testing-library/dom": "8.13.0",
    "@testing-library/user-event": "13.5.0",
    "@testing-library/vue": "6.6.0",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/compiler-sfc": "3.2.37",
    "bumpp": "8.1.0",
    "eslint": "^8.13.0",
    "jsdom": "19.0.0",
    "tsup": "6.1.2",
    "typescript": "4.7.3",
    "vitest": "0.15.1",
    "vue": "^3.2.36"
  },
  "tsup": {
    "name": "vue-use-hotkeys",
    "format": [
      "cjs",
      "esm"
    ],
    "dts": true,
    "clean": true
  },
  "publishConfig": {
    "access": "public"
  }
}
