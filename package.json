{
  "name": "vue-use-hotkeys",
  "description": "Vue composition for using keyboard shortcuts in components",
  "version": "0.1.1",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mitscherlich/vue-use-hotkeys.git"
  },
  "homepage": "https://github.com/Mitscherlich/vue-use-hotkeys#readme",
  "bugs": "https://github.com/Mitscherlich/vue-use-hotkeys/issues",
  "author": "Mitscherlich <mitscherlich36@gmail.com>",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "prepublishOnly": "pnpm run build",
    "example:dev": "vite ./example -c vite.config.js",
    "example:build": "vite build ./example -c vite.config.js"
  },
  "dependencies": {
    "hotkeys-js": "^3.8.7"
  },
  "devDependencies": {
    "@testing-library/dom": "8.11.3",
    "@testing-library/user-event": "13.5.0",
    "@testing-library/vue": "6.5.1",
    "@vitejs/plugin-vue": "2.2.4",
    "@vitejs/plugin-vue-jsx": "1.3.8",
    "@vue/compiler-sfc": "3.2.31",
    "jsdom": "19.0.0",
    "tsup": "5.11.13",
    "typescript": "4.6.2",
    "vite": "2.8.5",
    "vitest": "0.5.8",
    "vue": "3.2.31"
  },
  "peerDependencies": {
    "vue": ">=2.7 || 3.x"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "prettier": {
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "tsup": {
    "name": "vue-use-hotkeys",
    "entry": [
      "./src/index.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "sourcemap": true,
    "clean": true,
    "dts": true
  }
}
