{
  "name": "vue-use-hotkeys",
  "version": "0.1.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "prepublishOnly": "pnpm run build",
    "example:dev": "vite ./example -c vite.config.js",
    "example:build": "vite build ./example -c vite.config.js"
  },
  "dependencies": {
    "hotkeys-js": "^3.8.7"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.1.0",
    "@vitejs/plugin-vue-jsx": "^1.3.3",
    "@vue/test-utils": "^2.0.0-rc.18",
    "tsup": "^5.11.11",
    "typescript": "^4.5.5",
    "vite": "^2.7.13",
    "vitest": "^0.1.27",
    "vue": "^3.2.28"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.4.4",
    "vue": ">=2.6.16 || 3.x"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "prettier": {
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "tsup": {
    "name": "vue-use-hotkeys",
    "entry": [
      "./src/index.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "sourcemap": true,
    "clean": true,
    "dts": true
  }
}
