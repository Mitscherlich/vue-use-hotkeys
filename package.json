{
  "name": "vue-use-hotkeys",
  "description": "Vue composition for using keyboard shortcuts in components",
  "version": "0.3.5",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "source": "./src/index.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mitscherlich/vue-use-hotkeys.git"
  },
  "homepage": "https://github.com/Mitscherlich/vue-use-hotkeys#readme",
  "bugs": "https://github.com/Mitscherlich/vue-use-hotkeys/issues",
  "author": "Mitscherlich <mitscherlich36@gmail.com>",
  "scripts": {
    "build": "tsup ./src/index.ts",
    "dev": "tsup --watch",
    "test": "vitest run",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp package.json && pnpm publish"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-rc.1",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "@m9ch/vhooks": "~0.1.0-beta.10",
    "hotkeys-js": "^3.12.0",
    "vue-demi": "^0.14.5"
  },
  "devDependencies": {
    "@m9ch/eslint-config-all": "~0.3.2",
    "@testing-library/dom": "9.3.1",
    "@testing-library/user-event": "14.4.3",
    "@testing-library/vue": "7.0.0",
    "@vitejs/plugin-vue-jsx": "3.0.2",
    "@vue/compiler-sfc": "3.3.4",
    "bumpp": "9.2.0",
    "eslint": "8.48.0",
    "jsdom": "22.1.0",
    "tsup": "7.2.0",
    "typescript": "5.2.2",
    "vitest": "0.34.3",
    "vite": "4.4.9",
    "vue": "3.3.4"
  },
  "tsup": {
    "name": "vue-use-hotkeys",
    "format": [
      "cjs",
      "esm"
    ],
    "dts": true,
    "clean": true
  }
}
